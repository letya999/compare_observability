# OpenAI API
OPENAI_API_KEY=sk-...

# Observability Providers (comma-separated list of active providers)
# Options: langsmith,langfuse,arize,opik,braintrust,laminar,agentops,evidently,logfire
OBSERVABILITY_PROVIDERS=langsmith,langfuse,opik

# =============================================================================
# LangSmith Configuration
# Docs: https://docs.smith.langchain.com/
# =============================================================================
# Required: Enable tracing (LANGSMITH_TRACING is preferred, LANGCHAIN_TRACING_V2 also works)
LANGSMITH_TRACING=true
LANGCHAIN_TRACING_V2=true
# Required: API key (LANGSMITH_API_KEY preferred, LANGCHAIN_API_KEY also works)
LANGCHAIN_API_KEY=ls-...
# Optional: Project name for organizing traces
LANGCHAIN_PROJECT=compare-observability
# Optional: Custom endpoint (for self-hosted or EU region)
# LANGSMITH_ENDPOINT=https://api.smith.langchain.com

# =============================================================================
# Langfuse Configuration
# Docs: https://langfuse.com/docs/sdk/python
# =============================================================================
# Required: API keys
LANGFUSE_PUBLIC_KEY=pk-...
LANGFUSE_SECRET_KEY=sk-...
# Optional: API endpoint (LANGFUSE_BASE_URL preferred, LANGFUSE_HOST also works)
# EU region (default): https://cloud.langfuse.com
# US region: https://us.cloud.langfuse.com
LANGFUSE_HOST=https://cloud.langfuse.com
# Optional: Environment tag for organizing traces
# LANGFUSE_ENVIRONMENT=development

# =============================================================================
# Arize Phoenix Configuration
# Docs: https://docs.arize.com/phoenix/
# =============================================================================
PHOENIX_API_KEY=...
PHOENIX_COLLECTOR_ENDPOINT=https://app.phoenix.arize.com

# =============================================================================
# Opik (Comet ML) Configuration
# Docs: https://www.comet.com/docs/opik/
# =============================================================================
# Required: API key
OPIK_API_KEY=...
# Required: Workspace name
OPIK_WORKSPACE=...
# Optional: Cloud API endpoint (default for Opik Cloud)
OPIK_URL_OVERRIDE=https://www.comet.com/opik/api
# Optional: Project name for organizing traces
# OPIK_PROJECT_NAME=compare-observability

# =============================================================================
# Braintrust Configuration
# =============================================================================
BRAINTRUST_API_KEY=...

# =============================================================================
# Laminar Configuration
# =============================================================================
LMNR_PROJECT_API_KEY=...

# =============================================================================
# AgentOps Configuration
# =============================================================================
AGENTOPS_API_KEY=...

# =============================================================================
# Evidently (cloud) Configuration
# =============================================================================
EVIDENTLY_API_KEY=...

# =============================================================================
# Pydantic Logfire Configuration
# =============================================================================
LOGFIRE_TOKEN=...
